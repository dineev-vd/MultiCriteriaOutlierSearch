@using OutliersLib
@using OutliersApp.Models
@namespace OutliersApp.Components

@*Имя*@
<div class="input-group" style="margin-bottom: 8px; margin-top:8px;">
    <div class="input-group-prepend">
        <span class="input-group-text">Имя</span>
    </div><input type="text" class="form-control" @bind="@ExternalFormModel.Name"/>
</div>
<div class="input-group" style="margin-bottom: 8px; margin-top:8px;">
    <div class="input-group-prepend">
        <span class="input-group-text">URL</span>
    </div><input type="text" class="form-control" @bind="@ExternalFormModel.Uri"/>
</div>

@code{

    [Parameter]
    public ExternalFormModel ExternalFormModel { get; set; }

}